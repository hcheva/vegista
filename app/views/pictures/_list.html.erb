<% if @pictures %>
 <div class="row"> 
        <% @pictures.each do |picture| %>
             <div class="col-md-2"> 
                  <div class="panel panel-default"> 
                        <div class="panel-heading preview">
                             <%= image_tag picture.image.url(), style: "width: 100%;" %> 
                        </div>
                  </div>
             </div>
       <% end %>
 </div>
 <% end %>